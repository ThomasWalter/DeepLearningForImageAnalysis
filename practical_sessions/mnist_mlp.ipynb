    "from keras.regularizers import l1, l2\n",
    "\n",
    "reg_weight = 0.01\n",
    "\n",
    "model = Sequential()\n",
    "model.add(Dense(\n",
    "        128,\n",
    "        kernel_regularizer=l1(reg_weight),\n",
    "        activation='relu', \n",
    "        input_shape=(x_train.shape[1],)))\n",
    "model.add(Dense(\n",
    "        128,\n",
    "        kernel_regularizer=l1(reg_weight),\n",
    "        activation='relu'))\n",
    "model.add(Dense(\n",
    "        num_classes,\n",
    "        kernel_regularizer=l1(reg_weight),\n",
    "        activation='softmax'))"
